<?xml version="1.0"?>

<launch>
   
  <!-- Start Gazebo with wg world running in (max) realtime -->
  <include file="$(find hector_gazebo_worlds)/launch/rolling_landscape_120m.launch"/>
  <!-- <include file="$(find hector_gazebo_worlds)/launch/plane.launch"/> -->


  <!-- Spawn simulated quadrotor uav with kinect and laser -->
  <include file="$(find hector_quadrotor_gazebo)/launch/spawn_quadrotor_with_vision_box.launch" />  

  <!-- <node pkg="hector_quadrotor_teleop" type="spinning_lidar_control" name="spinning_lidar_control" /> -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find hector_quadrotor_demo)/rviz_cfg/vision_box.rviz"/>


</launch>
